{
  "name": "swv2",
  "version": "1.0.0",
  "description": "SwellNetwork V2",
  "scripts": {
    "fork:mainnet": "hardhat node --fork https://eth-mainnet.alchemyapi.io/v2/$(grep ALCHEMY_API_KEY .env | cut -d '=' -f2)",
    "test:inte": "hardhat test test/inte/*",
    "test:unit": "hardhat test test/unit/*",
    "prepare": "husky install",
    "coverage:inte": "hardhat coverage --testfiles 'test/inte/*.js' ",
    "coverage:unit": "hardhat coverage --testfiles 'test/unit/*.js' --network localhost"
  },
  "keywords": [
    "ETH2"
  ],
  "author": "SwellNetwork",
  "license": "BUSL-1.1",
  "bugs": {
    "url": "https://github.com/SwellNetwork/v2-core/issues"
  },
  "homepage": "https://github.com/SwellNetwork/v2-coree",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@uniswap/v3-sdk": "^3.8.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^4.0.0-alpha.0",
    "ethers": "^5.6.1",
    "hardhat": "^2.9.1",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-deploy": "^0.11.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-tracer": "^1.1.0-rc.3",
    "husky": "^7.0.4",
    "is-svg": "^4.3.2",
    "lint-staged": "^12.3.1",
    "mocha": "^9.2.2",
    "prettier": "^2.6.0",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "semantic-release": "^19.0.3",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.7.0",
    "typechain": "^7.0.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@balancer-labs/v2-pool-weighted": "^2.0.1",
    "@chainlink/contracts": "^0.4.0",
    "@chainlink/test-helpers": "^0.0.7-alpha",
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/contracts-upgradeable": "^4.5.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@uniswap/v3-sdk": "^3.8.2",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.0",
    "base64-sol": "^1.1.0",
    "cypress": "^9.3.1",
    "jsbi": "^4.3.0",
    "google-spreadsheet": "^3.0.14"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write test/**/*.js"
    ],
    "*.sol": "solhint --fix contracts/**/*.sol"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    }
  }
}